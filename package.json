{
  "name": "@beepbeepgo/semantic-release",
  "version": "2.2.15",
  "description": "A shared nodejs library for managing semantic-release.",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/beepbeepgo/public/libraries/npm/nodejs-semantic-release-config.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/beepbeepgo/public/libraries/npm/nodejs-semantic-release-config/issues"
  },
  "homepage": "https://gitlab.com/beepbeepgo/public/libraries/npm/nodejs-semantic-release-config#readme",
  "main": "src/",
  "files": [
    "src",
    "dist"
  ],
  "author": "beepbeepgo",
  "scripts": {
    "audit": "npm audit --production --audit-level=high",
    "semantic-release": "semantic-release",
    "test": "npx jest",
    "coverage": "npm test -- --coverage --group=unit --coverageReporters=cobertura --reporters=jest-junit",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "devDependencies": {
    "@beepbeepgo/eslint-config-beepbeepgo-common": "2.0.1",
    "eslint-plugin-react": "7.32.2",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "@codedependant/semantic-release-docker": "^4.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.1",
    "@semantic-release/npm": "^10.0.2",
    "@semantic-release/release-notes-generator": "^11.0.0",
    "multi-semantic-release": "^3.0.2",
    "semantic-release": "^21.0.0",
    "semantic-release-helm3": "^2.3.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
